{
  "name": "compilation-info-webpack-plugin",
  "version": "0.1.0",
  "description": "Plugin allowing to render compilation information as additional outputs.",
  "main": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "init": "npm install",
    "dev": "webpack --mode development -w",
    "prod": "webpack --mode production",
    "prepublishOnly": "npm run prod",
    "test": "jest --no-cache --testEnvironment node"
  },
  "author": "Lukáš Novotný",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kukkimonsuta/compilation-info-webpack-plugin.git"
  },
  "keywords": [
    "webpack"
  ],
  "dependencies": {
    "handlebars": "^4.0.12"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.39",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/webpack": "^4.4.17",
    "jest": "^23.6.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "webpack": "^4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/test/framework/preprocessor.js"
    },
    "testRegex": "/test/.*\\.ts$"
  }
}
